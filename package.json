{
  "name": "@4s1/avatar-generator",
  "version": "2.3.0",
  "description": "Avatar Generator",
  "bugs": {
    "url": "https://gitlab.com/4s1/avatar-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/avatar-generator.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "avatar-generator": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "ts-node src/index.ts",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo No tests available",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "prepare": "husky install"
  },
  "prettier": "@4s1/eslint-config",
  "dependencies": {
    "@4s1/toolbox": "1.2.3",
    "canvas": "2.8.0"
  },
  "devDependencies": {
    "@4s1/eslint-config": "3.1.2",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/node": "14.17.16",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "prettier": "2.4.1",
    "standard-version": "9.3.1",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "publishConfig": {
    "@4s1:registry": "https://gitlab.com/api/v4/projects/29205044/packages/npm/"
  }
}
